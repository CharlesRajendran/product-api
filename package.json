{
  "name": "product-api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "launch": "docker-compose --verbose up",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "NODE_ENV=test && jest --passWithNoTests --coverage --verbose",
    "test:coverage": "jest --coverage",
    "migrate": "sequelize-cli db:migrate",
    "rollback": "sequelize-cli db:migrate:undo",
    "migrate-and-start": "npm run migrate && npm start",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@hapi/boom": "^9.1.2",
    "cls-hooked": "^4.2.2",
    "csv-parse": "^4.15.4",
    "debug": "~4.3.1",
    "dotenv-flow": "^3.2.0",
    "express": "~4.17.1",
    "express-graphql": "^0.12.0",
    "express-rate-limit": "^5.2.6",
    "express-slow-down": "^1.4.0",
    "faker": "^5.5.3",
    "got": "^11.8.2",
    "graphql": "^15.5.0",
    "helmet": "^4.5.0",
    "hpp": "^0.2.3",
    "joi": "^17.4.0",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "redis": "^3.1.1",
    "sequelize": "^6.6.2",
    "slugify": "^1.5.0",
    "supertest": "^6.1.3",
    "swagger-jsdoc": "6.1.0",
    "swagger-ui-express": "^4.1.6",
    "umzug": "^2.3.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "sequelize-cli": "^6.2.0",
    "standard-version": "^9.2.0"
  }
}
